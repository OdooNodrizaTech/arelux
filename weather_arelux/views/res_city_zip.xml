<?xml version="1.0" encoding="utf-8"?>
<odoo>   
    <record id="weather_arelux_city_zip_tree" model="ir.ui.view">
        <field name="name">weather_arelux_city_zip_tree</field>
        <field name="model">res.city.zip</field>
        <field name="inherit_id" ref="base_location.city_zip_tree" />
        <field name="priority" eval="20"/>                
        <field name="arch" type="xml">
            <field name="city_id" position="after">
                <field name="weather_station_uuid" readonly="1"/>                                
            </field>                                                
        </field>
    </record>                           
</odoo>
